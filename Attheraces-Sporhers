    "from selenium import webdriver\n",
    "from webdriver_manager.chrome import ChromeDriverManager\n",
    "from selenium.webdriver.common.keys import Keys\n",
    "import time\n",
    "from selenium.webdriver.common.action_chains import ActionChains\n",
    "\n",
    "driver = webdriver.Chrome(r\"C:\\Users\\USER\\Documents\\Scraping_waghera\\chromedriver.exe\")\n",
    "\n",
    "from selenium.webdriver.support.ui import WebDriverWait\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.support import expected_conditions as EC\n",
    "\n",
    "from googleapiclient.discovery import build\n",
    "from googleapiclient.errors import HttpError\n",
    "from google.oauth2 import service_account\n",
    "\n",
    "from selenium.webdriver.support.ui import Select\n",
    "\n",
    "a = ActionChains(driver)\n",
    "\n",
    "driver.get(\"https://www.attheraces.com/results\")\n",
    "driver.implicitly_wait(10)\n",
    "driver.maximize_window()\n",
    "\n",
    "interested_courses = ['ascot','bangor-on-dee','bath','brighton','chepstow','chester','doncaster','fakenham','ffos las','fontwell','hereford','hexham','lingfield','newcastle','newton abbot','plumpton','ripon','sedgefield','southwell','uttoxeter','windsor','wolverhampton','worcester','yarmouth']\n",
    "\n",
    "time.sleep(3)\n",
    "data = []\n",
    "for day in range(2):\n",
    "    \n",
    "    if (day==0):\n",
    "        previous = driver.find_element_by_xpath(\"//button[@class='button button--small datepicker__control datepicker__control--prev']\")\n",
    "        previous.click()\n",
    "\n",
    "        time.sleep(2)\n",
    "        \n",
    "    else:\n",
    "        race_courses = driver.find_elements_by_xpath(\"//div[@id='tab-race-nav--uk']/div/div/div/div/div\")\n",
    "\n",
    "        for i,race_course in enumerate(race_courses):\n",
    "            \n",
    "\n",
    "            course_name = driver.find_element_by_xpath(f\"//div[@id='tab-race-nav--uk']/div/div/div/div/div[{i+1}]/label/span\").text.lower().strip()\n",
    "\n",
    "            if course_name in interested_courses:\n",
    "\n",
    "                races = driver.find_elements_by_xpath(f\"//div[@id='tab-race-nav--uk']/div/div/div/div/div[{i+1}]/div/div/ul/li\")\n",
    "\n",
    "\n",
    "                for j,rac in enumerate(races):\n",
    "                    race = rac.find_element_by_tag_name('a').get_attribute('href')\n",
    "                    print(race)\n",
    "                    data.append(race)\n",
    "\n",
    "    driver.find_element_by_tag_name('body').send_keys(Keys.CONTROL + Keys.HOME)\n",
    "\n",
    "    date_input = driver.find_element_by_xpath(\"//input[@id='race-nav-datepicker']\")\n",
    "    a.move_to_element(date_input).perform()\n",
    "\n",
    "    previous = driver.find_element_by_xpath(\"//button[@class='button button--small datepicker__control datepicker__control--prev']\")\n",
    "    previous.click()\n",
    "\n",
    "    time.sleep(2)\n",
    "\n",
    "print(data)\n",
    "\n",
    "comp_data = []\n",
    "\n",
    "for link in data:\n",
    "    driver.get(link)\n",
    "    sectional_times = driver.find_element_by_xpath(\"//ul[@class='tabs__links tabs__links--grey js-tabs__links']/li[2]/a\")\n",
    "    if 'Sectional Times' in sectional_times.text:\n",
    "\n",
    "        WebDriverWait(driver, 20).until(EC.element_to_be_clickable((By.XPATH, \"//ul[@class='tabs__links tabs__links--grey js-tabs__links']/li[2]/a\"))).click()\n",
    "        \n",
    "        race_time = driver.find_element_by_xpath(\"//h2[@class='h4 flush']/b\").text\n",
    "        details1 = driver.find_element_by_xpath(\"//h2[@class='h4 flush']\").text.split(' ')\n",
    "        racecourse_name = details1[1]\n",
    "        date = details1[2] + ' ' + details1[3] + ' ' + details1[4]\n",
    "        print(racecourse_name)\n",
    "        print(race_time)\n",
    "        print(date)\n",
    "        \n",
    "        total_rows = driver.find_elements_by_xpath(\"//div[@class='card card-column__fixed js-card-column__fixed']/div[2]/div\")\n",
    "        for k in range(len(total_rows)):\n",
    "            \n",
    "            time.sleep(5)\n",
    "            horse_name = driver.find_element_by_xpath(f\"//div[@class='card card-column__fixed js-card-column__fixed']/div[2]/div[{k+1}]/div/div[3]/div/div/h2/a\").text   \n",
    "            print(horse_name)\n",
    "\n",
    "            all_time = driver.find_elements_by_xpath(f\"//div[@class='scrollbar__content js-scrollbar__content']/div[2]/div[{k+1}]/div/div\")\n",
    "            print(len(all_time))\n",
    "            \n",
    "            inner_list = []\n",
    "            for s in range(len(all_time)):\n",
    "                \n",
    "                if s<(len(all_time)-3):\n",
    "                    times = driver.find_element_by_xpath(f\"//div[@class='scrollbar__content js-scrollbar__content']/div[2]/div[{k+1}]/div/div[{s+4}]\")\n",
    "                    print(times.text.split('\\n')[0])\n",
    "                    if (s == (len(all_time)-4)):\n",
    "                        if (len(all_time)>6):\n",
    "                            horizontal_bar_width = driver.find_element_by_xpath('//div[@class=\"js-scrollbar__track scrollbar__track\"]').rect['width']\n",
    "                            slider = driver.find_element_by_xpath('//div[@class=\"js-scrollbar__track scrollbar__track\"]')\n",
    "                            ActionChains(driver).click_and_hold(slider).move_by_offset(horizontal_bar_width/2, 0).release().perform()\n",
    "                        time.sleep(2)\n",
    "                      \n",
    "                        minutes = float(times.text.split('\\n')[0].split(' ')[0].replace('m',''))\n",
    "                        if 's' in times.text:\n",
    "                            seconds = float(times.text.split('\\n')[0].split(' ')[1].replace('s',''))\n",
    "                            conv_time = round((minutes*60+seconds),2)\n",
    "                            print(conv_time)\n",
    "                        else:\n",
    "                            conv_time = round((minutes*60),2)\n",
    "                            print(conv_time)\n",
    "#                         if (s==0):\n",
    "                    \n",
    "#             comp_data.append([date,racecourse_name,race_time,horse_name,conv_time])\n",
    "#                     inner_list.append(date)\n",
    "#                     inner_list.append(racecourse_name)\n",
    "#                     inner_list.append(race_time)\n",
    "#                     inner_list.append(horse_name)\n",
    "#                     inner_list.append(conv_time)\n",
    "#                     inner_list.append(times)\n",
    "#             comp_data.append(inner_list)\n",
    "        \n",
    "        print('Sectional Times')\n",
    "        \n",
    "        \n",
    "        \n",
    "        print(\"-------------------------------------------------------------\")\n",
    "    print(len(races))\n",
    "    print(course_name)\n",
    "    \n",
    "SERVICE_ACCOUNT_FILE = 'keys.json'\n",
    "SCOPES = ['https://www.googleapis.com/auth/spreadsheets']\n",
    "\n",
    "creds = None\n",
    "creds = service_account.Credentials.from_service_account_file(\n",
    "        SERVICE_ACCOUNT_FILE, scopes=SCOPES)\n",
    "\n",
    "SAMPLE_SPREADSHEET_ID = '19gNgMJPeriXMJ2mRnBrulMO1apYnBBbBBGHZMhwVF2M'\n",
    "\n",
    "\n",
    "service = build('sheets', 'v4', credentials=creds)\n",
    "\n",
    "# Call the Sheets API\n",
    "sheet = service.spreadsheets()\n",
    "\n",
    "# Write\n",
    "request = sheet.values().update(spreadsheetId=SAMPLE_SPREADSHEET_ID,\n",
    "                            range=\"Sheet3!A2\", valueInputOption=\"USER_ENTERED\", body={\"values\":comp_data}).execute()\n",
    "\n",
    "\n"
